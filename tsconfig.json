{
	"compilerOptions": {
		"composite": true,
		"paths": {
			"components/*": [
				"./src/components/*"
			],
			"icons/*": [
				"./node_modules/react-icons/*"
			],
			"pages/*": [
				"./src/pages/*"
			],
			"state/*": [
				"./src/state/*"
			],
			"types/*": [
				"./src/types/*"
			],
			"middlewares/*": [
				"./src/middlewares/*"
			],
			"hooks/*": [
				"./src/hooks/*"
			],
			"utils/*": [
				"./src/utils/*"
			],
			"widgets/*": [
				"./src/widgets/*"
			],
			"api/*": [
				"./src/api/*"
			],
			"utils/constants/*": [
				"./src/utils/constants/*"
			],
			"context/* ": [
				"./src/context/*"
			],
		},
		"target": "es6",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"outDir": "./dist",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": false,
		"module": "esnext",
		"rootDir": ".",
		"baseUrl": "src",
		"noImplicitReturns": false,
		"noImplicitAny": false,
		"importHelpers": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": false,
		"jsx": "react-jsx",
		"noFallthroughCasesInSwitch": true,
		"downlevelIteration": true
	},
	"typeRoots": [
		"node_modules/@types",
		"src/@types"
	],
	"exclude": [
		"node_modules",
		"build",
		"scripts"
	],
	"include": [
		"src/**/*",
		"src/utis/**/*",
		"src/state/**/*",
		"./tailwind.config.js",
		"sw.js",
	]
}